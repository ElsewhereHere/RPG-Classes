{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.paragonia_classes.entangle": {
      "initial_state": "grow",
      "states": {
        "grow": {
          "animations": ["grow"],
          "transitions": [
            {
              "shrink": "q.property('entangle:is_grabbing') == false"
            }
          ]
        },
        "shrink": {
          "animations": ["shrink"]
        }
      }
    }
  }
}
